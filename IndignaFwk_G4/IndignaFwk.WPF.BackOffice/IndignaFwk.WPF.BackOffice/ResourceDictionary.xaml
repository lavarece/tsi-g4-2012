<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<ControlTemplate x:Key="TextBoxControlTemplate1" TargetType="{x:Type TextBox}">
		<TextBox Margin="0" TextWrapping="Wrap" Background="#FFC8C8C8" BorderBrush="#FF00B4D6" FontStyle="Italic" SelectionBrush="Red" />
	</ControlTemplate>
	<ControlTemplate x:Key="ButtonControlTemplate_Login" TargetType="{x:Type Button}">
		<Button x:Name="boton_generico" Content="Iniciar Sesion" Margin="0,0,-1.046,-1">
			<Button.Background>
				<LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
					<GradientStop Color="#FFF3F3F3" Offset="0"/>
					<GradientStop Color="#FF85FFF1" Offset="1"/>
				</LinearGradientBrush>
			</Button.Background>
		</Button>
	</ControlTemplate>
	<ControlTemplate x:Key="LabelControlTemplate1" TargetType="{x:Type Label}"/>
	<ControlTemplate x:Key="PasswordBoxControlTemplate1" TargetType="{x:Type PasswordBox}">
		<PasswordBox Margin="0" Background="#FFC8C8C8" BorderThickness="1" BorderBrush="#FF00B4D6">
			<PasswordBox.Effect>
				<DropShadowEffect/>
			</PasswordBox.Effect>
		</PasswordBox>
	</ControlTemplate>
	<!-- Resource dictionary entries should be defined here. -->
</ResourceDictionary>