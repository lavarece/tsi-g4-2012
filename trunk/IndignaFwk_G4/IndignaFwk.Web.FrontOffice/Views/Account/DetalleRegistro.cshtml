@model IndignaFwk.Web.FrontOffice.Models.RegistroModel

@{
    ViewBag.Title = @ViewBag.GrupoSite.Nombre + " - Login";
    Layout = "~/Content/" + ViewBag.GrupoSite.Layout.NombreLayout + "/layout.cshtml";
}

<h3>Usuario registrado correctamente</h3>

<fieldset>
    <legend>Usuario Registrado</legend>
    
    <br/>
    
    <div class="display-label">Nombre:/div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Nombre)
    </div>

    <br/>

    <div class="display-label">Descripción:</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Descripcion)
    </div>

    <br/>

    <div class="display-label">Correo:<div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Email)
    </div>

</fieldset>

<div class="areaBotones">
    <ul>    
        <li>
            <div class="boton">
                <a href="@Url.Action("Index", "Home")">Volver al Inicio</a>
            </div>       
        </li>        
    </ul>     
</div>


